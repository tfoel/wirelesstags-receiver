{
  "name": "wirelesstags-receiver",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "moment": "^2.19.0",
    "pretty-error": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "fix": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" --fix .",
    "test": "node --napi-modules --no-warnings node_modules/.bin/jest",
    "test-watch": "node --napi-modules --no-warnings node_modules/.bin/jest --watch",
    "build": "node tools/build.js",
    "build-watch": "node tools/build.js --watch",
    "docker-install": "docker-compose r --rm --no-deps api yarn install",
    "docker-upgrade": "docker-compose run --rm --no-deps api yarn upgrade",
    "docker-test": "docker-compose run api yarn test",
    "docker-test-watch": "docker-compose run api yarn test-watch",
    "deploy": "node tools/publish.js",
    "start": "babel-node src/server.js --presets env"
  }
}
